$("ul.errors").html("")
<% if @county.errors.any? %>
    <% @county.errors.full_messages.each do |message| %>
        $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
    <% end %>
<% else %>
    $(".county-index").html("<%= escape_javascript(render 'index') %>")
    $("#county-modal").modal("hide")
<% end %>