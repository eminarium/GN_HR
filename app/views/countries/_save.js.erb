$("ul.errors").html("")
<% if @country.errors.any? %>
    <% @country.errors.full_messages.each do |message| %>
        $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
    <% end %>
<% else %>
    $(".country-index").html("<%= escape_javascript(render 'index') %>")
    $("#country-modal").modal("hide")
<% end %>