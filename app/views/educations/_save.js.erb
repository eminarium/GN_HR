$("ul.errors").html("")
<% if @education.errors.any? %>
    <% @education.errors.full_messages.each do |message| %>
        $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
    <% end %>
<% else %>
    $(".education-index").html("<%= escape_javascript(render 'index') %>")
    $("#education-modal").modal("hide")
<% end %>