$("ul.errors").html("")
<% if @warning.errors.any? %>
    <% @warning.errors.full_messages.each do |message| %>
        $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
    <% end %>
<% else %>
    $(".warning-index").html("<%= escape_javascript(render 'index') %>")
    $("#warning-modal").modal("hide")
<% end %>
