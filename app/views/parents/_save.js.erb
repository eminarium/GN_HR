$("ul.errors").html("")
<% if @parent.errors.any? %>
    <% @parent.errors.full_messages.each do |message| %>
        $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
    <% end %>
<% else %>
    $(".parent-index").html("<%= escape_javascript(render 'index') %>")
    $("#parent-modal").modal("hide")
<% end %>