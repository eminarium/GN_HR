$("ul.errors").html("")
<% if @nationality.errors.any? %>
    <% @nationality.errors.full_messages.each do |message| %>
        $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
    <% end %>
<% else %>
    $(".nationality-index").html("<%= escape_javascript(render 'index') %>")
    $("#nationality-modal").modal("hide")
<% end %>